{
  "name": "dunkelfalke80 Easy On The Eyes",
  "type": "dark",
  "colors": {
        "editor.background": "#101520",
        "editor.foreground": "#C0C0C0",
        "editorCursor.foreground": "#FFFFFF",
        "editor.lineHighlightBackground": "#2A2D31",
        "editorLineNumber.foreground": "#3A3F44",
        "editor.selectionBackground": "#3D4A56",
        "editor.inactiveSelectionBackground": "#3D4A5660",
        "editorIndentGuide.background1": "#404040",
        "editorWhitespace.foreground": "#101520",
        "activityBar.background": "#101520",
        "activityBar.foreground": "#C0C0C0",
        "activityBarBadge.background": "#67D8EF",
        "activityBarBadge.foreground": "#FFFFFF",
        "sideBar.background": "#23272E",
        "sideBar.foreground": "#C0C0C0",
        "sideBarTitle.foreground": "#C0C0C0",
        "sideBarSectionHeader.background": "#1B1E23",
        "statusBar.background": "#23272E",
        "statusBar.foreground": "#C0C0C0",
        "statusBar.debuggingBackground": "#FF6633",
        "statusBar.noFolderBackground": "#1B1E23",
        "titleBar.activeBackground": "#23272E",
        "titleBar.activeForeground": "#C0C0C0",
        "titleBar.inactiveBackground": "#23272E",
        "titleBar.inactiveForeground": "#C0C0C0",
        "menubar.selectionForeground": "#C0C0C0",
        "menubar.selectionBackground": "#2A2D31",
        "menu.foreground": "#C0C0C0",
        "menu.background": "#1B1E23",
        "menu.selectionForeground": "#FFFFFF",
        "menu.selectionBackground": "#3D4A56",
        "menu.separatorBackground": "#404040",
        "editorGroupHeader.tabsBackground": "#23272E",
        "editorGroup.border": "#2A2D31",
        "tab.activeBackground": "#1B1E23",
        "tab.activeForeground": "#C0C0C0",
        "tab.inactiveBackground": "#23272E",
        "tab.inactiveForeground": "#808080",
        "tab.border": "#2A2D31"
  },
  "tokenColors": [
          {
            "scope": [
              "comment",
              "comment.block.documentation",
              "punctuation.definition.comment"
            ],
            "settings": {
              "foreground": "#33AA33",
              "fontStyle": "italic"
            }
          },
          {
            "scope": [
              "constant.numeric",
              "number",
              "constant.numeric.hex",
              "constant.numeric.decimal",
              "constant.numeric.binary",
              "constant.numeric.octal",
              "constant.numeric.float"
            ],
            "settings": {
              "foreground": "#E5C100"
            }
          },
          {
            "scope": [              
              "string.quoted.single",
              "string.quoted.double",
              "string.quoted.triple",
              "string.interpolated"
            ],
            "settings": {
              "foreground": "#876FA8"
            }
          },
          {
            "scope": [
              "string"                            
            ],
            "settings": {
              "foreground": "#577386"
            }
          },
          {
            "scope": [              
              "variable.interpolation"
            ],
            "settings": {
              "foreground": "#DF3079"
            }
          },
          {
            "scope": [
            "meta.decorator"
          ],
            "settings": {
              "foreground": "#FF6633"
            }
          },
          {
            "scope": [
              "punctuation.definition.constant.numeric.hex"
            ],
            "settings": {
              "foreground": "#E5C100"
            }
          },
          {
            "scope": [
              "entity.name.class",
              "entity.name.type.class",
              "support.type",
              "support.class",
              "entity.name.type",
              "meta.type.name",
              "storage.type.annotation"
            ],
            "settings": {
              "foreground": "#4E90D5" // Brighter blue for classes
            }
          },
          {
            "scope": [
              "entity.name.function",
              "support.function"
            ],
            "settings": {
              "foreground": "#67D8EF"
            }
          },
          {
            "scope": [
              "variable",
              "meta.object-literal.key",
              "meta.function-call.arguments"
            ],
            "settings": {
              "foreground": "#D0D0D0"
            }
          },
          {
            "scope": [
              "keyword.operator",
              "keyword.other",
              "keyword"
            ],
            "settings": {
              "foreground": "#4E90D5"
            }
          },
          {
            "scope": [
              "keyword.control",
              "storage.type",
              "keyword",
              "storage.modifier",
              "storage.type.function"
            ],
            "settings": {
              "foreground": "#0099FF"
            }
          },
          {
            "scope": [
              "keyword.control.import",
              "keyword.control.from",
              "keyword.control.as",
              "keyword.control.del",
              "keyword.control.global",
              "keyword.control.nonlocal"
            ],
            "settings": {
              "foreground": "#33CCFF"
            }
          },
          {
            "scope": [
              "support.function",
              "support.variable",
              "variable.language",
              "support.constant"
            ],
            "settings": {
              "foreground": "#67D8EF"
            }
          },
          {
            "scope": [
              "punctuation",
              "punctuation.separator",
              "punctuation.terminator"
            ],
            "settings": {
              "foreground": "#FFFFFF"
            }
          }
  ]
}

